<h1 id="page_title">Projects I've written</h1>
<div id="article_wrapper" class="skinny_wrapper">
  <% @projects.each do |project| %>
    <div class="article">
      <p class="date"><%= project.updated_at.strftime("%d %B") %></p>
      <h2><a href="<%= project_path(project.id) %>"><%= project.title %></a></h2>
      <hr>
    </div>
  <% end %>
  <%= paginate @projects %>
</div>
